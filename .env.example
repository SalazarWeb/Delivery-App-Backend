DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=postgres
DB_NAME=delivery_db

# JWT Configuration
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRATION=7d

# Server Configuration
PORT=3000

# CORS Configuration
# En desarrollo: usa '*' para permitir todos los orígenes
# O especifica múltiples orígenes separados por coma:
# CORS_ORIGIN=http://localhost:19006,http://localhost:19000,http://192.168.1.100:19000
# En producción: especifica solo los orígenes permitidos
CORS_ORIGIN=*

# NODE_ENV (development, production, test)
NODE_ENV=development
